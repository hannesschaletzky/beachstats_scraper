{
  "name": "beachstats_scraper",
  "version": "1.0.0",
  "description": "Beach Volleyball Statistics Scraper",
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "setup-test": "ts-node src/db/setup-test-db.ts",
    "test": "npm run setup-test && env-cmd -f .env.test npm run migrate up && env-cmd -f .env.test jest --config",
    "migrate": "node-pg-migrate",
    "build": "rimraf ./build && tsc"
  },
  "keywords": [],
  "author": "Hannes Schaletzky",
  "license": "MIT",
  "dependencies": {
    "@types/jest": "^29.2.3",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^3.0.6",
    "app-root-path": "^3.1.0",
    "dotenv": "^16.0.0",
    "env-cmd": "^10.1.0",
    "got-scraping": "^3.2.12",
    "jest": "^29.3.1",
    "jsdom": "^20.0.1",
    "node-cron": "^3.0.2",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.8.0",
    "rimraf": "^3.0.2",
    "tiny-typed-emitter": "^2.1.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.9.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  }
}
